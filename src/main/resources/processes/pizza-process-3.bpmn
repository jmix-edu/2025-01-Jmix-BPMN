    <serviceTask id="Activity_0a2nz7a" name="send email to accountant" flowable:type="jmix-send-email">
      <extensionElements>
        <flowable:field name="to">
          <flowable:string>jane@pizzaexample.com</flowable:string>
        </flowable:field>
        <flowable:field name="subject">
          <flowable:string>Task assigned</flowable:string>
        </flowable:field>
        <flowable:field name="content">
          <flowable:string>Task assigned</flowable:string>
        </flowable:field>
        <flowable:field name="contentType">
          <flowable:string>text/plain; charset=UTF-8</flowable:string>
        </flowable:field>
      </extensionElements>
      <incoming>Flow_0jv4s4x</incoming>
    </serviceTask>
    <sequenceFlow id="Flow_1jvjgy4" sourceRef="Activity_0ujid86" targetRef="Activity_02y1kqp" />
    <scriptTask id="Activity_0ujid86" name="Add greetings" scriptFormat="groovy">
      <incoming>Flow_0s4g9wb</incoming>
      <outgoing>Flow_1jvjgy4</outgoing>
      <script>def messageWithGreeting = "Hi, " + pizzaEater.firstName + "! " + message   
execution.setVariable("message",  messageWithGreeting) </script>
    </scriptTask>
